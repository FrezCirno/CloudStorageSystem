<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>CloudStorage.Server</RootNamespace>
    <AssemblyName>CloudStorage.Server</AssemblyName>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Views.fs" />
    <Compile Include="FileMetaEntity.fs" />
    <Compile Include="Db.fs" />
    <Compile Include="Service.fs" />
    <Compile Include="Handlers.fs" />
    <Compile Include="Router.fs" />
    <Compile Include="Startup.fs" />
    <Compile Include="Program.fs" />
    <EmbeddedResource Remove="tmp\**" />
    <PackageReference Include="giraffe" Version="4.1.0" />
    <PackageReference Include="mysqlconnector" Version="1.3.2" />
    <PackageReference Include="SQLProvider" Version="1.2.1" />
    <PackageReference Update="FSharp.Core" Version="5.0.1" />
  </ItemGroup>
</Project>